<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.0"/>
  <object class="GtkDialog" id="custom-tables-dialog">
    <property name="title" translatable="yes">Custom Tables</property>
    <property name="default-width">400</property>
    <property name="default-height">300</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="dialog-vbox">
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkButton" id="create-report-button">
            <property name="label" translatable="yes">Создать отчет</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
